{
  "redirects": [
    {
      "source": "/v/1.0/:path*",
      "destination": "/:path*"
    },
    {
      "source": "/v/2.0/:path*",
      "destination": "/2.0/:path*"
    },
    {
      "source": "/v/1.0/",
      "destination": "/"
    },
    {
      "source": "/v/2.0/",
      "destination": "/2.0/"
    }
  ]
}
